{
  "name": "ng-compiler",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "ng serve",
    "start:server": "tsx watch server/index.ts",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:snapshot": "tsx scripts/build-snapshot.ts"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.3",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@webcontainer/api": "^1.6.1",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "ioredis": "^5.4.1",
    "marked": "^17.0.3",
    "monaco-editor": "^0.55.1",
    "rxjs": "~7.8.0",
    "sql.js": "^1.14.0",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2"
  },
  "devDependencies": {
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@webcontainer/snapshot": "^0.1.0",
    "angular-eslint": "21.2.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.2",
    "tsx": "^4.19.0",
    "typescript-eslint": "8.50.1"
  }
}
